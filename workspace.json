{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"],
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts",
              "apps/api/**/*.spec.ts",
              "apps/api/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/api"]
        }
      }
    },
    "api-interface": {
      "root": "libs/api-interface",
      "sourceRoot": "libs/api-interface/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api-interface/**/*.ts",
              "libs/api-interface/**/*.spec.ts",
              "libs/api-interface/**/*.spec.tsx",
              "libs/api-interface/**/*.spec.js",
              "libs/api-interface/**/*.spec.jsx",
              "libs/api-interface/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interface/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/api-interface"]
        }
      }
    },
    "my-site": {
      "root": "apps/my-site",
      "sourceRoot": "apps/my-site/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/my-site",
            "index": "apps/my-site/src/index.html",
            "main": "apps/my-site/src/main.tsx",
            "polyfills": "apps/my-site/src/polyfills.ts",
            "tsConfig": "apps/my-site/tsconfig.app.json",
            "assets": [
              "apps/my-site/src/favicon.ico",
              "apps/my-site/src/assets"
            ],
            "styles": ["apps/my-site/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack",
            "buildLibsFromSource": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/my-site/src/environments/environment.ts",
                  "with": "apps/my-site/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "my-site:build",
            "proxyConfig": "apps/my-site/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "my-site:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-site/**/*.js",
              "apps/my-site/**/*.jsx",
              "apps/my-site/**/*.ts",
              "apps/my-site/**/*.tsx",
              "apps/my-site/**/*.spec.ts",
              "apps/my-site/**/*.spec.tsx",
              "apps/my-site/**/*.spec.js",
              "apps/my-site/**/*.spec.jsx",
              "apps/my-site/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/my-site/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/my-site"]
        }
      }
    },
    "my-site-e2e": {
      "root": "apps/my-site-e2e",
      "sourceRoot": "apps/my-site-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-site-e2e/cypress.json",
            "tsConfig": "apps/my-site-e2e/tsconfig.e2e.json",
            "devServerTarget": "my-site:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-site:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-site-e2e/src/**/*.ts",
              "apps/my-site-e2e/src/**/*.js"
            ]
          }
        }
      }
    },
    "ui-header": {
      "root": "libs/ui-header",
      "sourceRoot": "libs/ui-header/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-header/**/*.js",
              "libs/ui-header/**/*.jsx",
              "libs/ui-header/**/*.ts",
              "libs/ui-header/**/*.tsx",
              "libs/ui-header/**/*.spec.ts",
              "libs/ui-header/**/*.spec.tsx",
              "libs/ui-header/**/*.spec.js",
              "libs/ui-header/**/*.spec.jsx",
              "libs/ui-header/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui-header/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/ui-header"]
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "my-site"
}
